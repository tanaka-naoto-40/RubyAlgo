<tr>
  <td>
    <%= user.id %>
  </td>
  <td>
    <%= user.line_user_id %>
  </td>
  <td>
    <%= link_to user.username, admin_user_path(user) %>
  </td>
  <td>
    <%= link_to "Edit", edit_admin_user_path(user), class: 'btn btn-success' %>
    <%= link_to "Delete", admin_user_path(user), class: 'btn btn-danger', data: { turbo_method: :delete, turbo_confirm: "本当に削除しますか？" } %> 
  </td>
</tr>
